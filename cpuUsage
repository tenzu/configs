#!/bin/bash
echo "=== on cu01 ==="
cat /home/coreApplication/current.log | grep cu01 | awk '{print $3 "\t" $4 "\t" $5 "\t" $1 "\t" $2}' | sort
ssh cu01 "cpuAlert"
echo "=== on cu02 ==="
cat /home/coreApplication/current.log | grep cu02 | awk '{print $3 "\t" $4 "\t" $5 "\t" $1 "\t" $2}' | sort
ssh cu02 "cpuAlert"
